{
  "name": "@ttpos/share-utils",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/nsiod/share.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/nsiod/share.git",
    "directory": "packages/utils"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@noble/ciphers": "catalog:prod",
    "@noble/curves": "catalog:prod",
    "@noble/hashes": "catalog:prod",
    "@scure/base": "catalog:prod",
    "@scure/bip32": "catalog:prod",
    "@scure/bip39": "catalog:prod",
    "eciesjs": "catalog:prod"
  },
  "devDependencies": {
    "@ttpos/share-tsconfig": "workspace:*",
    "@types/node": "catalog:dev",
    "size-limit": "catalog:dev",
    "tslib": "catalog:dev",
    "tsup": "catalog:dev",
    "typescript": "catalog:dev"
  }
}
